@model IEnumerable<Landing_page.Models.Vendedor>

@{
    ViewBag.Title = "Listado";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Listado</h2>

<div class="create-item">
    <p>
        @Html.ActionLink("Inicio", "Index")
    </p>
</div> 

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id_vendedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Categoria_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombres_vendedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellidos_vendedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Correo_vendedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Celular_vendedor)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id_vendedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_tienda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Categoria_tienda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombres_vendedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellidos_vendedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Correo_vendedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Celular_vendedor)
        </td>
        <td>
            @* @Html.ActionLink("Edit", "Edit", new { id=item.Id_vendedor }) |
        @Html.ActionLink("Details", "Details", new { id=item.Id_vendedor }) |
        @Html.ActionLink("Delete", "Delete", new { id=item.Id_vendedor })*@
        </td>
    </tr>
}

</table>
